{
    "name": "molajo/log",
    "type": "molajo-bundle",
    "description": "Simple, clean Log API for PHP applications to Get, Set, Remove, getMultiple, setMultiple, removeMultiple, and Clear Log for APC, Database, Dummy, File, Memlogd, Memory, Redis, Winlog, and xLog handlers.",
    "keywords": ["Log", "Molajo", "PSR-0"],
    "homepage": "http://github.com/Molajo/Log",
    "license": "MIT",
    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@gmail.com",
            "homepage": "http://AmyStephen.github.io"
        },
        {
            "name": "Other Contributors",
            "homepage": "http://github.com/Molajo/Log/contributors"
        }
    ],
    "require": {
        "php": ">=5.3"
    },

    "support": {
        "email": "hello@molajo.org",
        "irc": "irc://irc.freenode.org/molajo"
    },
    "autoload": {
        "psr-0": {
            "Molajo\\Log": "./"
        }
    },

    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
