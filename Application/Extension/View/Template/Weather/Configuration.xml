<?xml version="1.0" encoding="utf-8"?>
<extension type="Templateview" version="1.0">
    <name>VIEW_TEMPLATE_WEATHER_NAME</name>
    <author>Amy Stephen</author>
    <create_date>January 2013</create_date>
    <copyright>Copyright (C) 2013 Amy Stephen. All rights reserved.</copyright>
    <license>MIT</license>
    <author_email>hello@molajo.org</author_email>
    <author_url>http://molajo.org</author_url>
    <version>1.0</version>
    <description>VIEW_TEMPLATE_WEATHER_DESCRIPTION</description>
    <model name="Weather" extends="Templateview">
        <customfields>
            <customfield name="parameters">
                <field name="city" type="char" null="0" default=""/>
                <field name="zip" type="char" null="0" default="10003"/>
                <field name="country" type="char" null="0" default="US"/>
                <field name="fahrenheit_or_celsius" type="char" null="0" default="F" values="C,F"/>
                <field name="language" type="char" null="0" default="en"/>
            </customfield>
        </customfields>
    </model>
</extension>
