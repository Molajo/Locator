<?xml version="1.0" encoding="utf-8"?>
<model
        name="LanguageService"
        table_name="#__extension_instances"
        criteria_catalog_type_id="6000"
        primary_key="id"
        name_key="title"
        primary_prefix="a"
        get_customfields="1"
        get_item_children="0"
        use_special_joins="1"
        check_view_level_access="0"
        process_plugins="0"
        use_pagination="1"
        extends="ExtensionInstances"
        data_object="Database">

    <customfields>
        <customfield name="parameters">
            <field name="name" type="char" default="English (United Kingdom)" null="0"/>
            <field name="tag" type="char" default="en-GB" null="0"/>
            <field name="rtl" type="boolean" default="0" null="0"/>
            <field name="locale"
                   type="char"
                   default="en_GB.utf8, en_GB.UTF-8, en_GB, eng_GB, en, english, english-uk, uk, gbr, britain, england, great britain, uk, united kingdom, united-kingdom"
                   null="0"/>
            <field name="first_day" type="integer" default="0" null="0" values="1,2,3,4,5,6,7"/>
        </customfield>
    </customfields>
</model>


