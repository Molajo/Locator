<?xml version="1.0" encoding="utf-8"?>
<extension type="System" version="1.0">
    <name>SYSTEM_LANGUAGES_NAME</name>
    <author>Amy Stephen</author>
    <create_date>January 2013</create_date>
    <copyright>Copyright (C) 2013 Amy Stephen. All rights reserved.</copyright>
    <license>MIT</license>
    <author_email>hello@niambie.org</author_email>
    <author_url>http://niambie.org</author_url>
    <version>1.0</version>
    <description>SYSTEM_LANGUAGES_DESCRIPTION</description>
    <model
            name="Languages"
            criteria_catalog_type_id="6000"
            criteria_extension_instance_id="6000"
            extends="RenderDatasource">
        <customfields>
            <customfield name="parameters">
                <field name="name" type="char" default="English (United Kingdom)" null="0"/>
                <field name="tag" type="char" default="en-GB" null="0"/>
                <field name="rtl" type="char" default="0" null="0"/>
                <field name="locale" type="char"
                       default="en_GB.utf8, en_GB.UTF-8, en_GB, eng_GB, en, english, english-uk, uk, gbr, britain, england, great britain, uk, united kingdom, united-kingdom"
                       null="0"/>
                <field name="first_day" type="integer" default="0" null="0" values="1,2,3,4,5,6,7"/>
            </customfield>
        </customfields>
    </model>
</extension>
