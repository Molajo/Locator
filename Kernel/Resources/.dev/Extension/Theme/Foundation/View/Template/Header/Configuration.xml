    <author>Amy Stephen</author>
    <create_date>January 2013</create_date>
    <copyright>Copyright (C) 2013 Amy Stephen. All rights reserved.</copyright>
    <license>MIT</license>
    <author_email>AmyStephen@niambie.com</author_email>
    <author_url>http://niambie.com</author_url>
    <version>1.0</version>
    <description>VIEW_TEMPLATE_HEADER_DESCRIPTION</description>
    <model name="Header" extends="TemplateviewsSystem">
        <customfields>
            <customfield name="parameters">
                <field name="header_logo" type="char" null="0" default=""/>
                <field name="link" type="char" null="1" default="http://Molajo.org"/>
            </customfield>
        </customfields>
    </model>
</extension>
